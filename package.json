{
	"name": "json-base64-vscode",
	"displayName": "json-base64-vscode",
	"description": "decode base64 to formatted json and encode json to base64",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/yuvraj-arch/json-base64-vscode.git"
	},
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:json-base64-vscode.decodeBas64ToJson",
		"onCommand:json-base64-vscode.encodeJsonToBase64"
	],
	"icon": "icon.png",
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-base64-vscode.decodeBas64ToJson",
				"title": "Decode Base64 To Json"
			},
			{
				"command": "json-base64-vscode.encodeJsonToBase64",
				"title": "Encode Json To Base64"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"build": "yarn run compile && tsc",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"prettier": "2.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
